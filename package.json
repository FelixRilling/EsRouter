{
    "name": "avenuejs",
    "version": "6.0.2",
    "description": "Avenue is an extremely small routing library",
    "main": "dist/avenue.common.js",
    "module": "dist/avenue.esm.js",
    "settings": {
        "input": "main.ts",
        "namespace": {
            "module": "Avenue",
            "file": "avenue"
        },
        "external": []
    },
    "scripts": {
        "start": "npm-run-all lint build test docs",
        "build": "rollup -c ./tasks/rollup.config.js",
        "build:iife": "rollup -c ./tasks/rollup.config.iife.js",
        "build:iife-min": "rollup -c ./tasks/rollup.config.iife-min.js",
        "watch": "rollup -c ./tasks/rollup.config.js --watch",
        "dist": "npm-run-all --parallel build build:iife build:iife-min",
        "lint": "npm-run-all --parallel lint:ts lint:es",
        "lint:ts": "tslint ./src/**/*.ts",
        "lint:es": "eslint ./spec/**/*.js",
        "fix": "npm-run-all fix:ts fix:es fix:prettier",
        "fix:ts": "tslint ./src/**/*.ts --fix",
        "fix:es": "eslint ./spec/**/*.js --fix",
        "fix:prettier": "prettier ./src/**/* ./spec/**/* --write",
        "docs": "jsdoc -c jsdoc.json"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/FelixRilling/avenuejs.git"
    },
    "keywords": [
        "routing",
        "router"
    ],
    "author": "Felix Rilling",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/FelixRilling/avenuejs/issues"
    },
    "homepage": "https://github.com/FelixRilling/avenuejs#readme",
    "browserslist": [
        "last 2 Chrome versions",
        "last 2 Firefox versions"
    ],
    "dependencies": {
        "lightdash": "^8.0.0"
    },
    "devDependencies": {
        "@babel/core": "^7.0.0-beta.51",
        "@babel/plugin-external-helpers": "^7.0.0-beta.51",
        "@babel/preset-env": "^7.0.0-beta.51",
        "docdash": "^0.4.0",
        "eslint": "^5.0.1",
        "jasmine": "^3.1.0",
        "jsdoc": "^3.5.5",
        "npm-run-all": "^4.1.3",
        "prettier": "^1.13.6",
        "rollup": "^0.62.0",
        "rollup-plugin-babel": "^4.0.0-beta.5",
        "rollup-plugin-node-resolve": "^3.3.0",
        "rollup-plugin-typescript2": "^0.15.1",
        "rollup-plugin-uglify-es": "0.0.1",
        "tslint": "^5.10.0",
        "typescript": "^2.9.2",
        "uglify-es": "^3.3.10"
    }
}
