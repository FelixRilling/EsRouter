{
    "name": "avenuejs",
    "version": "v3.7.0",
    "description": "Avenue is an ES6 based, modern and small library for for DOM-routing built for advanced single-page navigation.",
    "main": "dist/avenue.js",
    "module": {
        "name": "Avenue",
        "id": "avenue"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/FelixRilling/Avenue.git"
    },
    "scripts": {
        "start": "npm run dist",
        "dist": "npm run sync  && npm run build && npm run uglify && npm run header",
        "build": "npm run bundle && npm run babel",
        "bundle": "rollup -c",
        "babel": "rollup -c ./rollup.babel.config.js",
        "uglify": "node ./tasks/uglify.js",
        "sync": "node ./tasks/sync.js",
        "header": "node ./tasks/header.js"
    },
    "keywords": [
        "router",
        "routing"
    ],
    "author": "Felix Rilling",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/FelixRilling/Avenue/issues"
    },
    "homepage": "https://github.com/FelixRilling/Avenue#readme",
    "devDependencies": {
        "babel-plugin-external-helpers": "^6.8.0",
        "babel-preset-es2015": "^6.14.0",
        "rollup": "^0.35.0",
        "rollup-plugin-babel": "^2.6.1",
        "rollup-plugin-commonjs": "^4.0.0",
        "rollup-plugin-node-resolve": "^2.0.0",
        "uglify-js": "^2.7.0"
    },
    "dependencies": {}
}
