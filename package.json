{
    "name": "avenuejs",
    "version": "v4.0.0",
    "description": "Avenue is a small and modern DOM-router designed for onpage routing.",
    "main": "dist/avenue.common.js",
    "namespace": {
        "module": "Avenue",
        "file": "avenue"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/FelixRilling/Avenue.git"
    },
    "scripts": {
        "start": "npm run dist",
        "dist": "npm run sync  && npm run build && npm run uglify && npm run header",
        "build": "npm run bundle && npm run babel",
        "bundle": "rollup -c",
        "babel": "rollup -c ./rollup.babel.config.js",
        "uglify": "node ./tasks/uglify.js",
        "sync": "node ./tasks/sync.js",
        "header": "node ./tasks/header.js"
    },
    "keywords": [
        "router",
        "routing"
    ],
    "author": "Felix Rilling",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/FelixRilling/Avenue/issues"
    },
    "homepage": "https://github.com/FelixRilling/Avenue#readme",
    "devDependencies": {
        "babel-plugin-external-helpers": "^6.18.0",
        "babel-preset-es2015": "^6.18.0",
        "gulp": "^3.9.1",
        "gulp-clean": "^0.3.2",
        "gulp-config-sync": "^1.0.2",
        "gulp-header": "^1.8.8",
        "gulp-jasmine": "^2.4.2",
        "gulp-rename": "^1.2.2",
        "gulp-sequence": "^0.4.6",
        "gulp-sourcemaps": "^2.2.0",
        "gulp-uglify": "^2.0.0",
        "pump": "^1.0.1",
        "rollup-plugin-babel": "^2.6.1",
        "rollup-stream": "^1.14.0",
        "vinyl-buffer": "^1.0.0",
        "vinyl-source-stream": "^1.1.0"
    },
    "dependencies": {}
}
