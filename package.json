        "type": "git",
        "url": "git+https://github.com/FelixRilling/esRouter.git"
    },
    "keywords": [
        "esRouter",
        "router",
        "es6"
    ],
    "author": "Felix Rilling",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/FelixRilling/esRouter/issues"
    },
    "homepage": "https://github.com/TheBinaryBee/FelixRilling#readme",
    "devDependencies": {
        "babel-plugin-array-includes": "^2.0.3",
        "babel-preset-es2015": "^6.14.0",
        "gulp": "^3.9.0",
        "gulp-babel": "^6.1.0",
        "gulp-rename": "^1.2.2",
        "gulp-shell": "^0.5.0",
        "gulp-uglify": "^2.0.0",
