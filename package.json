{
    "name": "avenuejs",
    "version": "v3.2.0",
    "description": "Avenue is an ES6 based, modern and small library for for DOM-routing built for advanced single-page navigation.",
    "main": "dist/avenue.js",
    "module": {
        "name": "Avenue",
        "id": "avenue"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/FelixRilling/Avenue.git"
    },
    "scripts": {
        "start": "npm run sync  && npm run bundle && npm run babel && npm run uglify",
        "sync": "node ./tasks/sync.js",
        "bundle": "rollup -c",
        "babel": "babel ./dist/es6 --out-dir ./dist",
        "uglify": "node ./tasks/uglify.js"
    },
    "keywords": [
        "dependencies",
        "services"
    ],
    "author": "Felix Rilling",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/FelixRilling/Avenue/issues"
    },
    "homepage": "https://github.com/FelixRilling/Avenue#readme",
    "devDependencies": {
        "babel-cli": "^6.14.0",
        "babel-preset-es2015": "^6.14.0",
        "rollup": "^0.34.0",
        "rollup-plugin-commonjs": "^3.3.0",
        "rollup-plugin-node-resolve": "^2.0.0",
        "uglify-js": "^2.7.0"
    },
    "dependencies": {}
}
